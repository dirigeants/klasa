{
  "name": "klasa",
  "version": "0.6.0-dev",
  "description": "Klasa: Croatian for 'class', is a class based remix on Komada.",
  "homepage": "https://klasa.js.org/",
  "bugs": {
    "url": "https://github.com/dirigeants/klasa/issues"
  },
  "license": "MIT",
  "author": "BDistin",
  "main": "dist/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dirigeants/klasa.git"
  },
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "test": "ava",
    "test:lint": "eslint --ext ts src test",
    "test:coverage": "npx nyc check-coverage --lines 0 --functions 0 --branches 0",
    "coverage": "npx nyc --require source-map-support/register npm test",
    "coverage:report": "npx nyc report --reporter=html",
    "lint": "eslint --fix --ext ts src test",
    "docs": "typedoc"
  },
  "dependencies": {
    "@klasa/cache": "^0.0.3",
    "@klasa/cron": "^0.0.1",
    "@klasa/duration": "^0.0.1",
    "@klasa/ratelimits": "^0.0.1",
    "@klasa/timestamp": "^0.0.1",
    "@klasa/type": "^0.0.1",
    "@klasa/utils": "^0.0.5",
    "fs-nextra": "^0.4.7"
  },
  "peerDependencies": {
    "@klasa/core": "^0.0.1",
    "@klasa/timer-manager": "0.0.1"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@klasa/core": "^0.0.1",
    "@klasa/dapi-types": "^0.1.1",
    "@klasa/timer-manager": "0.0.1",
    "@types/node": "14",
    "@types/node-fetch": "^2.5.7",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "ava": "^3.8.2",
    "eslint": "^6.8.0",
    "eslint-config-klasa": "dirigeants/klasa-lint",
    "klasa": "^0.5.0",
    "nyc": "^15.0.1",
    "source-map-support": "^0.5.19",
    "typedoc": "^0.17.7",
    "typescript": "^4.0.0-dev.20200522"
  },
  "engines": {
    "node": ">=12.6.0"
  },
  "ava": {
    "files": [
      "test/**/*.ts",
      "!test/lib"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  }
}
